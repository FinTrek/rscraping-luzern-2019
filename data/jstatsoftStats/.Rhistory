list_files_path <-  list.files(folder, pattern = "0.*", full.names = TRUE)
length(list_files)
# import pages and extract content
authors <- character()
title <- character()
statistics <- character()
numViews <- numeric()
datePublish <- character()
for (i in 1:length(list_files_path)) {
